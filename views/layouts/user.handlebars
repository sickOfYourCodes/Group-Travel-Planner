<html lang="en">

  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta
      name="description"
      content="A group travel planner, so you and your group are all on the same page!"
    />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!--style reset links -->
    <link rel="reset" href="/assets/css/reset.css" />
    <!--font links -->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css?family=Raleway%22%3E"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css%22%3E"
    />
    <link
      rel="stylesheet"
      href="https://unicons.iconscout.com/release/v4.0.0/css/line.css"
    />
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css" />
    <link rel="stylesheet" href="/assets/css/style.css" />
  </head>

  <body>
    <!--navbar begins here-->
    <div class="navWrapper w3-top">
      <div class="w3-bar w3-blue" id="myNavbar">
        <div class="selector w3-right w3-hide-small">
          <a href="/" class="active w3-bar-item w3-button"><i
              class="uil uil-estate"
            ></i>Home</a>
          <a href="/about" class="active w3-bar-item w3-button"><i
              class="uil uil-users-alt"
            ></i>About</a>
          <a href="/faq" class="active w3-bar-item w3-button"><i
              class="uil uil-comment-alt-notes"
            ></i>FAQ</a>
          <a href="/dashboard" class="active w3-bar-item w3-button"><i
              class="uil uil-estate"
            ></i>Dashboard</a>
          <a href="/" class="active w3-bar-item w3-button Loutout"><i
              class="uil uil-sign-in-alt"
            ></i>Logout</a>
        </div>
        <!-- Hide right-floated links on small screens and replace them with a menu icon -->

        <a
          href="javascript:void(0)"
          class="w3-bar-item w3-button w3-right w3-hide-large w3-hide-medium"
          onclick="w3_open()"
        >
          <i class="fa fa-bars"></i>
        </a>
      </div>
    </div>

    <!-- Sidebar on small screens when clicking the menu icon -->
    <nav
      class="w3-sidebar w3-bar-block w3-black w3-card w3-animate-left w3-hide-medium w3-hide-large"
      style="display:none"
      id="mySidebar"
    >
      <a
        href="javascript:void(0)"
        onclick="w3_close()"
        class="w3-bar-item w3-button w3-large w3-padding-16"
      >Close Ã—</a>
      <a
        href="/about"
        onclick="w3_close()"
        class="w3-bar-item w3-button"
      >About</a>
      <a href="/faq" onclick="w3_close()" class="w3-bar-item w3-button">Faq</a>
      <a href="/dashboard" onclick="w3_close()" class="w3-bar-item w3-button">Dashboard</a>
      <a
        href="/"
        onclick="w3_close()"
        class="w3-bar-item w3-button Logout"
      >Logout</a>
    </nav>

    <!--navbar ends here -->

    <!--handlebar section -->
    {{{body}}}

    <!--handlebar section Ends -->

    <footer
      id="footer"
      class="p-4 justify-center h-48 mt-24 flex gap-4 flex-col bg-blue-400"
    >
      <p class="text-center text-white text-xl font-semibold">
        &copy; 2023 Travel Planner
      </p>
      <ul class="flex flex-row justify-center gap-2">
        <li
          id="Travel-Planner"
          class="p-1 hover:opacity-15 duration-300 ease-out hover:bg-blue-300 bg-blue-400 rounded-md footer-items"
        >
          <a href="/">Travel Planner</a>
        </li>
        <li
          id="About-Us"
          class="p-1 hover:opacity-15 duration-300 ease-out hover:bg-blue-300 bg-blue-400 rounded-md footer-items"
        >
          <a href="/about">About Us</a>
        </li>
        <li
          id="FAQ"
          class="p-1 hover:opacity-15 duration-300 ease-out hover:bg-blue-300 bg-blue-400 rounded-md footer-items"
        >
          <a href="/faq">FAQ</a>
        </li>
        <li
          id="Login"
          class="p-1 hover:opacity-15 duration-300 ease-out hover:bg-blue-300 bg-blue-400 rounded-md footer-items"
        >
          <a href="/dashboard">Dashboard</a>
        </li>
        <li
          class="p-1 hover:opacity-15 duration-300 ease-out hover:bg-blue-300 bg-blue-400 rounded-md footer-items Logout"
        >
          <a href="/">Logout</a>
        </li>
      </ul>
      <div class="inner">
        <ul class="copyright">
          <li>&copy; sickOfOurCodes!</li>
          <li>Design: cool coders club.</li>
        </ul>
      </div>
    </footer>
    <!--script tag for navbar -->
    <script src="/assets/js/navbar.js"></script>
    <script src="/assets/js/main.js"></script>
  </body>

</html>